# TMDb API Configuration
TMDB_API_KEY=your_tmdb_api_key_here
TMDB_BASE_URL=https://api.themoviedb.org/3

# Server Configuration
HOST=0.0.0.0
PORT=8010

# LinkedIn enrichment (linkedin-api)
# Основная авторизация по логину и паролю
LINKEDIN_USERNAME=your_linkedin_email
LINKEDIN_PASSWORD=your_linkedin_password

# Альтернатива: путь к JSON с cookie или сами cookie в виде JSON
# LINKEDIN_COOKIES_PATH=/app/config/linkedin_cookies.json
# LINKEDIN_COOKIES_JSON={"li_at":"...","JSESSIONID":"..."}

# Дополнительные параметры
# LINKEDIN_REQUEST_INTERVAL=1.5
# LINKEDIN_MAX_RESULTS=3
# LINKEDIN_CONCURRENCY=1
# LINKEDIN_PROXY=socks5://127.0.0.1:9050
